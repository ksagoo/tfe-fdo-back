def clone_and_rename_client_lists(session, base_url, config_id, version, access_group, section):
    print(f"[INFO] Cloning and renaming client lists from Security Policy Template (DEBUG ONLY)")
    summary = {
        "processed": 0,
        "successful": 0,
        "failed": 0,
        "results": []
    }

    # Fetch all client lists including deleted/inactive ones
    url = f"{base_url}/client-list/v1/lists?includeDeprecated=true&includeNetworkLists=true"
    try:
        response = session.get(url)
        response.raise_for_status()
        client_lists = response.json().get("content", [])
    except Exception as e:
        print(f"[ERROR] Failed to fetch client lists: {e}")
        return summary

    # Build a map of client list names and whether they are deletable
    existing_lists = {}
    for cl in client_lists:
        cl_name = cl.get("name", "")
        deletable = cl.get("availableActions", {}).get("DELETE", {}).get("available", False)
        existing_lists[cl_name] = deletable

    # Iterate through all client lists to find template lists
    for cl in client_lists:
        try:
            name = cl.get("name", "")
            list_id = cl.get("listId")
            list_type = cl.get("listType")
            list_format = cl.get("type")

            if not name.startswith("Security Policy Template"):
                continue

            suffix = name.replace("Security Policy Template", "").strip(" -:")
            new_name = f"SC-{access_group}-{suffix}"

            existing_state = existing_lists.get(new_name)

            if existing_state is False:
                print(f"[SKIP] Existing list '{new_name}' found and still in use (not deletable)")
                continue

            # Debug: would attempt to clone
            print(f"[DRYRUN] Would clone '{name}' to '{new_name}' using list ID: {list_id}")
            summary["processed"] += 1
            summary["results"].append({
                "source": name,
                "target": new_name,
                "status": "dryrun"
            })

        except Exception as e:
            print(f"[ERROR] Failed during dryrun processing of list '{name}': {e}")
            summary["processed"] += 1
            summary["failed"] += 1
            summary["results"].append({
                "status": "error",
                "source": name,
                "error": str(e)
            })

    return summary
